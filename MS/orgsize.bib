journal = {Ecology Letters},
keywords = {Coevolution,competition,macroevolution,mutualism,phylogenetic tree,trait evolution},
number = {1},
pages = {17--27},
pmid = {25349102},
title = {{Predicting rates of interspecific interaction from phylogenetic trees}},
url = {http://dx.doi.org/10.1111/ele.12384$\backslash$nhttp://onlinelibrary.wiley.com/store/10.1111/ele.12384/asset/ele12384.pdf?v=1{\&}t=i9123ygk{\&}s=2e8c44d65d60b6b5fb3ef5e2f3f4f57bb488cdd9},
