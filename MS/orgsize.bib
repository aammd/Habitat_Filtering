journal = {Ecology},
keywords = {Biodiversity,Carnivores,Complementarity,Ecosystem function,Phylogenetic diversity, PD,Taxonomic distinctness, TD},
number = {10},
pages = {2180--2187},
pmid = {24358704},
title = {{Effects of predator richness on prey suppression: A meta-analysis}},
volume = {94},
year = {2013}
}
